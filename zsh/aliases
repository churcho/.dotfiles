#!/usr/bin/env zsh

alias blog="cd ~/Development/blog"
alias dev="cd ~/Development"
alias dotfiles="cd ~/.dotfiles"
alias dropbox="cd ~/Dropbox"
alias ea="vim ~/.zsh/aliases"
alias eal="vim ~/.zsh/aliases.local"
alias ef="vim ~/.zsh/funcs"
alias em="mix ecto.migrate"
alias ev="vim ~/.vimrc"
alias ez="vim ~/.zshrc"
alias gbl="git --no-pager branch -l"
alias home="cd $HOME"
alias journal="dropbox && cd journal"
alias jts="cd ~/Development/jekyll-tailwind-starter"
alias l1="ls -1"
alias lib="cd ~/libcasts"
alias m="FZF_DEFAULT_OPTS=--reverse mvim ."
alias mdkir="mkdir"
alias planet="cd ~/Development/planet_ex"
alias sn="mvim -c 'SimplenoteList'"
alias sz="source ~/.zshrc"
alias tree="tree | less"
alias unwrap-last-commit="git reset --soft HEAD~1"
alias v="vim ."
alias vi="vim ."

source $HOME/.zsh/aliases.local
